package main;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.lang.reflect.Array;
import java.util.ArrayList;
import java.util.Comparator;
import java.util.StringTokenizer;

public class Main {

    public ArrayList<Integer> solution(int n, int m, int arrayF[], int arrayS[]) {
        ArrayList<Integer> answer = new ArrayList<>();

        for(int i = 0; i< n; i++){
           answer.add(arrayF[i]);
        }
        for(int i =0; i<m; i++){
            answer.add(arrayS[i]);
        }
        answer.sort(Comparator.naturalOrder());
        return answer;
    }

    public static void main(String[] args) throws IOException {
        Main T = new Main();

        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

        int n = Integer.parseInt(br.readLine());
        int[] arrayF = new int[n];
        StringTokenizer st = new StringTokenizer(br.readLine());
        for (int i = 0; i < n; i++) {
            arrayF[i] = Integer.parseInt(st.nextToken(" "));
        }

        int m = Integer.parseInt(br.readLine());
        int[] arrayS = new int[m];
        StringTokenizer st2 = new StringTokenizer(br.readLine());
        for (int i = 0; i < m; i++) {
            arrayS[i] = Integer.parseInt(st2.nextToken());
        }


        System.out.println(T.solution(n, m, arrayF, arrayS));

    }
}
